{
  "name": "dnd-dynamic-sound-electron-2",
  "productName": "dnd-dynamic-sound-electron-2",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "prettify": "prettier --write 'src/**/*.{tsx,ts,jsx,js}'"
  },
  "keywords": [],
  "author": {
    "name": "teoboley",
    "email": "teo751@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "electron-squirrel-startup": "^1.0.0",
    "immer": "^5.3.2",
    "lodash": "^4.17.15",
    "rc-slider": "^9.1.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^10.2.1",
    "styled-components": "^5.0.0",
    "tone": "^14.5.28"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.49",
    "@electron-forge/maker-deb": "6.0.0-beta.49",
    "@electron-forge/maker-rpm": "6.0.0-beta.49",
    "@electron-forge/maker-squirrel": "6.0.0-beta.49",
    "@electron-forge/maker-zip": "6.0.0-beta.49",
    "@electron-forge/plugin-webpack": "6.0.0-beta.49",
    "@hot-loader/react-dom": "^16.11.0",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/lodash": "^4.14.149",
    "@types/rc-slider": "^8.6.5",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/styled-components": "^4.4.2",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "css-loader": "^3.0.0",
    "electron": "8.0.0",
    "eslint": "6.x",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.x",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "node-loader": "^0.6.0",
    "prettier": "^1.19.1",
    "react-hot-loader": "^4.12.19",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "dnd_dynamic_sound_electron_2"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  }
}
